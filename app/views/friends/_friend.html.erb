<% if friend.user == current_user %>
  <tr id="<%= dom_id friend %>">
    <td>
      <%= link_to friend.first_name + " " + friend.last_name, friend, class:"text-decoration-none" %>
    </td>
    <td>
      <%= friend.email %>
    </td>
    <td>
      <%= friend.phone %>
    </td>
    <td>
      <%= friend.instagram %>
    </td>
    <td>
      <%= link_to "Edit", edit_friend_path(friend), class:"btn btn-outline-success btn-sm" %>
    </td>
    <td>
      <%= button_to "Delete", friend, method: :delete, class:"btn btn-outline-danger btn-sm" %>
    </td>
  </tr>
<% end %>
